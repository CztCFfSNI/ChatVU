{
  "version": 2,
  "builds": [
    {
      "src": "./backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./frontend/src/App.jsx",
      "use": "@vercel/react"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/index.html"
    }
  ]
}
